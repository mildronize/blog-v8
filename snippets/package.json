    "typescript": "~5.6.2",
    "vite": "^6.0.2"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@iarna/toml": "^2.2.5",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "tiny-glob": "^0.2.9",
    "valibot": "^1.0.0-beta.9"
  }
}